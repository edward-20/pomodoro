{
  "name": "Pomodoro Timer",
  "description": "Use a pomodoro timer to help you study",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["alarms", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Pomodoro",
    "default_popup": "popup.html"
  }
}
